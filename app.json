{
  "expo": {
    "name": "gaulois-app",
    "slug": "gaulois-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gaulois-lg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/gaulois-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/College Block.otf.otf",
            "./assets/fonts/Kadwa.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
