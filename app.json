{
  "expo": {
    "name": "Gaulois",
    "slug": "gaulois-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gaulois-lg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/gaulois-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/College Block.otf.otf",
            "./assets/fonts/Kadwa.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gaulois-lg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lilian_mrbl.gauloisapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fb94d905-7a6b-48b8-9359-24247ee6c53f"
      }
    },
    "owner": "lilian_mrbl"
  }
}
